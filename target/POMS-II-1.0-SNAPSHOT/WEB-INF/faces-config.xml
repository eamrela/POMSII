<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        
        
        
        <navigation-rule>
            <from-view-id>/app/business_provider/create_activity.xhtml</from-view-id>
            <navigation-case>
            <from-action>#{activityController.prepareCreate}</from-action>
            <from-outcome>com.vodafone.poms.ii.entities.Activity[ activityId=null ]</from-outcome>
            <to-view-id>/app/business_provider/create_activity.xhtml</to-view-id>
          </navigation-case>
        </navigation-rule>
        
        <navigation-rule>
            <from-view-id>/app/finance_admin/create_asp_po.xhtml</from-view-id>
            <navigation-case>
            <from-action>#{aspPoController.prepareCreate}</from-action>
            <from-outcome>com.vodafone.poms.ii.entities.AspPo[ poNumber=null ]</from-outcome>
            <to-view-id>/app/finance_admin/create_asp_po.xhtml</to-view-id>
          </navigation-case>
        </navigation-rule>
        
        <navigation-rule>
            <from-view-id>/app/finance_admin/create_v_po.xhtml</from-view-id>
            <navigation-case>
            <from-action>#{vendorPoController.prepareCreate}</from-action>
            <from-outcome>com.vodafone.poms.ii.entities.VendorPo[ poNumber=null ]</from-outcome>
            <to-view-id>/app/finance_admin/create_v_po.xhtml</to-view-id>
          </navigation-case>
        </navigation-rule>
    </application>
</faces-config>
